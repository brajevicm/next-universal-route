{
  "name": "next-universal-route",
  "version": "1.7.0",
  "description": "Universal Next.js Route",
  "main": "./cjs/index.js",
  "module": "./es/index.js",
  "modules.root": "./es",
  "types": "./cjs/index.d.ts",
  "author": "Miloš Brajević <brajevicms@gmail.com>",
  "repository": "brajevicm/next-universal-route",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "jest --config jest.config.json && codecov",
    "test:watch": "jest --config jest.config.json --watchAll",
    "build": " rimraf ./dist && npm run build:es && npm run build:cjs",
    "build:es": "tsc --module es2015 --target es5 --outDir es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir cjs"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "express",
    "node",
    "universal",
    "routes",
    "router",
    "route",
    "routing",
    "ssr",
    "server-rendering"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/lru-cache": "^5.1.1",
    "@types/next": "^9.0.0",
    "@types/node": "^16.11.12",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.2",
    "request-ip": "^2.1.3"
  }
}
