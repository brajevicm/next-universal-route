{
  "name": "next-universal-route",
  "version": "1.9.1",
  "description": "Universal Next.js Route",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "modules.root": "./dist",
  "types": "./dist/index.cjs.d.ts",
  "author": "Miloš Brajević <brajevicms@gmail.com>",
  "repository": "brajevicm/next-universal-route",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "jest --config jest.config.json && codecov",
    "test:watch": "jest --config jest.config.json --watchAll",
    "build": " rimraf ./dist && npm run build:es && npm run build:cjs",
    "build:es": "tsc --target es5 --outFile dist/index.es.js",
    "build:cjs": "tsc --target es5 --outFile dist/index.cjs.js"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "express",
    "node",
    "universal",
    "routes",
    "router",
    "route",
    "routing",
    "ssr",
    "server-rendering"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lru-cache": "^7.6.1",
    "@types/next": "^9.0.0",
    "@types/node": "^17.0.23",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.3"
  }
}
