{
  "name": "next-universal-route",
  "version": "1.0.6",
  "description": "Universal Next.js Route",
  "main": "./cjs/index.js",
  "module": "./es/index.js",
  "modules.root": "./es",
  "types": "./cjs/index.d.ts",
  "author": "Miloš Brajević <brajevicms@gmail.com>",
  "repository": "brajevicm/next-universal-route",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "jest --config jest.config.json && codecov",
    "test:watch": "jest --config jest.config.json --watchAll",
    "build": " rimraf ./dist && npm run build:es && npm run build:cjs",
    "build:es": "tsc --module es2015 --target es5 --outDir es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir cjs"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "express",
    "node",
    "universal",
    "routes",
    "router",
    "route",
    "routing",
    "ssr",
    "server-rendering"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/next": "^9.0.0",
    "@types/node": "^12.12.18",
    "@types/path-to-regexp": "^1.7.0",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "jest": "^24.9.0",
    "next": "^9.1.1",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "next": ">=9",
    "react": "^16"
  },
  "dependencies": {
    "path-to-regexp": "^6.1.0",
    "qs": "^6.9.1"
  }
}
